pipeline {
  agent {
    docker {image 'node:16-alpine'}
  }
  stages {
    stage('build') {
      steps {
        sh'node version'
      }
    }
    stage('Deploy') {
      steps {
        sh'kubectl apply -f deployment.yml'
      }
    }
    stage('test') {
      steps {
        sh'node --version'
      }
    }
  }
}
